{
  "name": "server",
  "version": "0.0.1",
  "prisma": {
    "seed": "ts-node prisma/seed"
  },
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "dotenv -e .env.development.local nest start",
    "start:dev": "dotenv -e .env.development.local nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "dotenv -e .env.test.local jest -i",
    "test:single": "dotenv -e .env.test.local jest",
    "test:watch": "dotenv -e .env.test.local jest --watch",
    "test:cov": "dotenv -e .env.test.local jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e .env.test.local jest e2e.spec",
    "db:seed": "dotenv -e .env.development.local pnpm prisma db seed",
    "gen-prisma": "dotenv -e .env.development.local pnpm prisma generate",
    "migrate:postgres": "dotenv -e .env.test.local -- pnpm prisma migrate dev --name postgres-init",
    "create-migrate": "dotenv -e .env.development.local pnpm prisma migrate dev --name",
    "deploy-migrate": "dotenv -e .env.development.local pnpm prisma migrate deploy",
    "deploy-test": "dotenv -e .env.test.local pnpm prisma migrate deploy"
  },
  "dependencies": {
    "@automapper/classes": "^8.7.4",
    "@automapper/core": "^8.7.4",
    "@automapper/nestjs": "^8.7.4",
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/mapped-types": "^1.1.0",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^8.0.0",
    "@prisma/client": "^4.0.0",
    "bcrypt": "^5.0.1",
    "cache-manager": "^4.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "lodash": "^4.17.21",
    "nestjs-real-ip": "^2.1.1",
    "nodemailer": "^6.7.8",
    "passport": "0.6.0",
    "passport-facebook-token": "^4.0.0",
    "passport-google-oauth-token": "^1.0.4",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "string-interpolation-js": "^1.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.6",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.7",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^4.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.1",
    "@types/nodemailer": "^6.4.5",
    "@types/passport-facebook-token": "^0.4.35",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/pluralize": "^0.0.29",
    "@types/supertest": "^2.0.12",
    "dotenv-cli": "^6.0.0",
    "jest": "28.1.2",
    "pluralize": "^8.0.0",
    "prisma": "^4.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "4.0.0"
  }
}
